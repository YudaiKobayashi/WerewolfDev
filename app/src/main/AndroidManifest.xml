<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="yudai.werewolfdev">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NumberOfPlayers" />
        <activity android:name=".PlayerNames" />
        <activity android:name=".SetRoles" />
        <activity android:name=".StandBy" />
        <activity android:name=".PlayRoles" />
        <activity android:name=".Conversation" />
        <activity android:name=".Vote" />
        <activity android:name=".Execution" />
        <activity android:name=".Result" />
        <activity android:name=".Score" />
    </application>

</manifest>